{
	"$schema": "https://docs.rome.tools/schemas/12.1.2/schema.json",
	"files": {
		"ignore": ["dist/**/*"]
	},
	"organizeImports": {
		"enabled": false
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"suspicious": {
				"noExplicitAny": "warn"
			}
		}
	},
	"formatter": {
		"enabled": true
	}
}
